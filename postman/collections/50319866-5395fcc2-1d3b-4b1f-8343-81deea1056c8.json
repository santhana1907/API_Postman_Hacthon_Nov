	"event": [
		{
			"listen": "prerequest",
			"script": {
				"id": "debc957c-146a-4123-a3b8-3be9cfe5e52c",
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"try {\r",
					"\r",
					"    if (typeof pm.variables.get('requestData') !== 'object') {\r",
					"        pm.variables.set('requestData', pm.iterationData.toObject());\r",
					"    }\r",
					"\r",
					"    const requestData = pm.variables.get('requestData');\r",
					"    \r",
					"    if (typeof requestData !== 'object' || !requestData.requests || !Array.isArray(requestData.requests)) {\r",
					"        console.log('Invalid data structure or no requests array found');\r",
					"        return;\r",
					"    }\r",
					"    if (Object.keys(requestData).length === 0) {\r",
					"        console.log('No external data provided/data set is empty');\r",
					"        return;\r",
					"    }\r",
					"    // Find current request data \r",
					"    const currentRequest = requestData.requests.find(({ request }) => request === pm.info.requestName);\r",
					"\r",
					"    if (!currentRequest) {\r",
					"        console.log(`Request '${pm.info.requestName}' has no data defined in JSON file.`);\r",
					"        return;\r",
					"    }\r",
					"\r",
					"    \r",
					"    if (!currentRequest.data || !Array.isArray(currentRequest.data) || currentRequest.data.length === 0) {\r",
					"        console.log(`No test data available for request '${pm.info.requestName}'`);\r",
					"        return;\r",
					"    }\r",
					"\r",
					"    // Get the next test case data\r",
					"    const scenarioData = currentRequest.data[0]; // Use first element instead of shift to avoid mutation issues\r",
					"\r",
					"    if (!scenarioData || typeof scenarioData !== 'object') {\r",
					"        console.log('Invalid test case data');\r",
					"        return;\r",
					"    }\r",
					"    \r",
					"    Object.entries(scenarioData).forEach(([key, value]) => {\r",
					"        if (value !== undefined && value !== null) {\r",
					"            pm.variables.set(key, value.toString());\r",
					"        }\r",
					"    });\r",
					"   \r",
					"    logTestCaseInfo(scenarioData);    \r",
					"    configureRequestURL(scenarioData);    \r",
					"    // Configure Authorization header\r",
					"    configureAuthorization(scenarioData);\r",
					"\r",
					"    // Remove the used test case from the data array\r",
					"    currentRequest.data = currentRequest.data.slice(1);\r",
					"    \r",
					"    // Update the requestData with modified data\r",
					"    pm.variables.set('requestData', requestData);\r",
					"\r",
					"    // Set up next iteration for the same request if more data exists\r",
					"    if (currentRequest.data.length > 0) {\r",
					"        pm.execution.setNextRequest(pm.info.requestName);\r",
					"    } else {\r",
					"        console.log(`All test cases completed for request: ${pm.info.requestName}`);\r",
					"    }\r",
					"\r",
					"} catch (error) {\r",
					"    console.error('Error in pre-request script:', error.message);\r",
					"    console.error('Stack trace:', error.stack);\r",
					"}\r",
					"\r",
					"// function to configure request URL\r",
					"function configureRequestURL(scenarioData) {\r",
					"    try {\r",
					"        const scenario = scenarioData.scenario || \"\";\r",
					"        const endpoint = scenarioData.endpoint || \"\";\r",
					"        \r",
					"        let baseUrl;\r",
					"\r",
					"        if (scenario.toLowerCase().includes(\"invalidbaseurl\")) {\r",
					"            baseUrl = pm.variables.get(\"invalidBase_URL\") || \"https://invalid-base-url.example.com\";\r",
					"            console.warn(\"Using invalid base URL for scenario:\", scenario);\r",
					"        } else {\r",
					"            baseUrl = pm.environment.get(\"baseURL\") || pm.collectionVariables.get(\"baseURL\");\r",
					"        }\r",
					"\r",
					"        if (!baseUrl) {\r",
					"            console.error(\"Base URL is not defined\");\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        // Clean up URL formatting\r",
					"        const cleanBaseUrl = baseUrl.replace(/\\/+$/, ''); // Remove trailing slashes\r",
					"        const cleanEndpoint = endpoint.replace(/^\\/+/, ''); // Remove leading slashes\r",
					"        \r",
					"        const newUrl = `${cleanBaseUrl}/${cleanEndpoint}`;\r",
					"        pm.request.url = newUrl;\r",
					"        console.log(\"Configured URL for scenario '\" + scenario + \"':\", newUrl);\r",
					"        \r",
					"    } catch (error) {\r",
					"        console.error(\"Error configuring URL:\", error.message);\r",
					"    }\r",
					"}\r",
					"\r",
					"// function to configure authorization\r",
					"function configureAuthorization(scenarioData) {\r",
					"    try {\r",
					"        const includeAuth = (scenarioData.auth || \"\").toString().toLowerCase();\r",
					"        \r",
					"        \r",
					"        pm.request.headers.remove(\"Authorization\");\r",
					"        \r",
					"        switch (includeAuth) {\r",
					"            case 'true':\r",
					"                const validToken = pm.environment.get('BearerToken') || pm.collectionVariables.get('BearerToken');\r",
					"                if (validToken) {\r",
					"                    pm.request.headers.add({ \r",
					"                        key: 'Authorization', \r",
					"                        value: `Bearer ${validToken}` \r",
					"                    });\r",
					"                    console.log(\"Added valid Authorization header\");\r",
					"                } else {\r",
					"                    console.warn(\"Valid BearerToken not found in environment/collection variables\");\r",
					"                }\r",
					"                break;\r",
					"                \r",
					"            case 'invalid':\r",
					"                const invalidToken = scenarioData.invalidToken || \"Invalid_Token_Value_12345\";\r",
					"                pm.request.headers.add({ \r",
					"                    key: 'Authorization', \r",
					"                    value: `Bearer ${invalidToken}` \r",
					"                });\r",
					"                console.log(\"Added invalid Authorization header for testing\");\r",
					"                break;\r",
					"                \r",
					"            case 'false':\r",
					"                console.log(\"No Authorization header added as per test case\");\r",
					"                break;\r",
					"                \r",
					"            default:\r",
					"                console.log(\"No specific Authorization configuration needed\");\r",
					"                break;\r",
					"        }\r",
					"        \r",
					"    } catch (error) {\r",
					"        console.error(\"Error configuring authorization:\", error.message);\r",
					"    }\r",
					"}\r",
					"\r",
					"// FUNCTION  MAIN FLOW \r",
					"function logTestCaseInfo(scenarioData) {\r",
					"    console.log(`--- Test Case: ${scenarioData.scenario || 'Unnamed Scenario'} ---`);\r",
					"    console.log(`Request: ${pm.info.requestName}`);\r",
					"    console.log(`Iteration: ${pm.info.iteration} of ${pm.info.iterationCount}`);\r",
					"    console.log(`Endpoint: ${scenarioData.endpoint}`);\r",
					"    console.log(`Expected Status: ${scenarioData.expectedStatusCode}`);\r",
					"    console.log('---');\r",
					"}\r",
					"\r",
					""
				]
			}
